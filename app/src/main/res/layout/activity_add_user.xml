<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.bignerdranch.android.personaltrainertracker.AddUserActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+ Add Photo"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/fragment"
        app:layout_constraintRight_toLeftOf="@+id/imageView3"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.545"/>

    <android.support.design.widget.TextInputLayout
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="181dp">

    </android.support.design.widget.TextInputLayout>

    <EditText
        android:id="@+id/editText3"
        android:layout_width="106dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="textImeMultiLine|textPersonName"
        android:text="First Name:"
        tools:layout_editor_absoluteY="86dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="8dp"/>

    <EditText
        android:id="@+id/editText4"
        android:layout_width="106dp"
        android:layout_height="49dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Last Name:"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText3"
        app:layout_constraintRight_toLeftOf="@+id/editText8"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <EditText
        android:id="@+id/editText6"
        android:layout_width="133dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="Email Address:"
        android:layout_marginTop="1dp"
        app:layout_constraintTop_toBottomOf="@+id/editText4"
        app:layout_constraintRight_toLeftOf="@+id/editText10"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <EditText
        android:id="@+id/editText7"
        android:layout_width="105dp"
        android:layout_height="41dp"
        android:ems="10"
        android:inputType="number"
        android:text="Expires:"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText11"
        app:layout_constraintRight_toLeftOf="@+id/textInputLayout2"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/button2"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.888"
        app:layout_constraintVertical_bias="0.0"/>

    <EditText
        android:id="@+id/editText11"
        android:layout_width="105dp"
        android:layout_height="41dp"
        android:ems="10"
        android:inputType="number"
        android:text="Credit Card:"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editText6"
        app:layout_constraintRight_toLeftOf="@+id/textInputLayout"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <android.support.design.widget.TextInputLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="251dp"
        tools:layout_editor_absoluteY="402dp">

    </android.support.design.widget.TextInputLayout>

    <EditText
        android:id="@+id/editText8"
        android:layout_width="251dp"
        android:layout_height="47dp"
        android:hint="hint"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="139dp"/>

    <EditText
        android:id="@+id/editText10"
        android:layout_width="227dp"
        android:layout_height="39dp"
        android:hint="hint"
        tools:layout_editor_absoluteX="157dp"
        tools:layout_editor_absoluteY="196dp"/>

    <EditText
        android:id="@+id/editText9"
        android:layout_width="249dp"
        android:layout_height="39dp"
        android:hint="hint"
        app:layout_constraintLeft_toRightOf="@+id/editText3"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"/>

    <android.support.design.widget.TextInputLayout
        android:layout_width="243dp"
        android:layout_height="43dp"
        tools:layout_editor_absoluteY="238dp"
        tools:layout_editor_absoluteX="135dp"
        android:id="@+id/textInputLayout">

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="hint"/>
    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.TextInputLayout
        android:layout_width="90dp"
        android:layout_height="44dp"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="287dp"
        android:id="@+id/textInputLayout2">

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="hint"/>
    </android.support.design.widget.TextInputLayout>

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickAddUser"
        android:text="+ Add Customer"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <fragment
        android:id="@+id/fragment"
        android:name="com.bignerdranch.android.personaltrainertracker.LoggedIn"
        android:layout_width="115dp"
        android:layout_height="23dp"

        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="7dp"
        tools:layout="@layout/fragment_logged_in"/>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="80dp"
        android:layout_height="84dp"
        app:srcCompat="@mipmap/contact_icon"
        tools:layout_editor_absoluteY="2dp"
        tools:layout_editor_absoluteX="152dp"/>

</android.support.constraint.ConstraintLayout>
